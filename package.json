{
  "name": "sticky-bottom",
  "version": "1.0.0",
  "description": "Sticky bottom library - vanilla JS",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --progress --config webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --env.production",
    "build": "cross-env NODE_ENV=production webpack --env.production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/sticky-bottom.git"
  },
  "keywords": ["sticky"],
  "author": "kunukn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunukn/sticky-bottom/issues"
  },
  "homepage": "https://github.com/kunukn/sticky-bottom#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
